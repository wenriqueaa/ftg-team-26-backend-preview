{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'none'; img-src 'self' https://ftg-team-26-backend-swagger.vercel.app; script-src 'self'"
        }
      ]
    }
  ]
}